    "copy-email-templates-to-build": "cp -rv src/email-service/templates build/email-service"
  },
  "devDependencies": {
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@types/joi": "^14.3.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.1",
    "@types/uuid": "^7.0.3",
    "aws-sdk": "^2.683.0",
    "ethers": "^5.0.31",
    "expo-server-sdk": "^3.6.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.8.0",
    "moment": "^2.24.0",
    "uuid": "^8.1.0"
  }
